{
    "manifest_version": 2,
    "name": "MeMe & Share",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"],
            "css": ["index.css"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "MeMe & Share"
    },

    "permissions": ["activeTab"]
}